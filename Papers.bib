@article{mariniAttractionComesMany2020,
	title = {Attraction comes from many sources: {Attentional} and comparative processes in decoy effects},
	volume = {15},
	issn = {1930-2975},
	shorttitle = {Attraction comes from many sources},
	url = {https://www.cambridge.org/core/product/identifier/S1930297500007889/type/journal_article},
	doi = {10.1017/S1930297500007889},
	abstract = {The attraction eﬀect emerges when adding a seemingly irrelevant option (decoy) to a binary choice shifts preference towards a target option. This suggests that choice behaviour is dynamic, i.e., choice values are developed during deliberation, rather than manifesting some pre-existing preference set. Whereas several models of multialternative and multiattribute decision making consider dynamic choice processes as crucial to explain the attraction eﬀect, empirically investigating the exact nature of such processes requires complementing choice output with other data. In this study, we focused on asymmetrically dominated decoys (i.e., decoys that are clearly dominated only by the target option) to examine the attentional and comparative processes responsible for the attraction eﬀect. Through an eye-tracker paradigm, we showed that the decoy option can aﬀect subjects’ preferences in two diﬀerent and not mutually exclusive ways: by focusing the attention on the salient option and the dominance attribute, and by increasing comparisons with the choice dominant pattern. Although conceptually and procedurally distinct, both pathways for decoy eﬀects produce an increase in preferences for the target option, in line with attentional and dynamic models of decision making. Eye-tracking data provide further details to the veriﬁcation of such models, by highlighting the context-dependent nature of attention and the development of similarity-driven competitive decisional processes.},
	language = {en},
	number = {5},
	urldate = {2023-03-09},
	journal = {Judgment and Decision Making},
	author = {Marini, Marco and Ansani, Alessandro and Paglieri, Fabio},
	month = sep,
	year = {2020},
	pages = {704--726},
	file = {Marini_et_al_2020.pdf:/Users/seanconway/Reading/Papers/Marini_et_al_2020.pdf:application/pdf},
}

@article{maylorSimilarityAttractionEffects2007b,
