@article{nosofskyRecommendationsCognitivePsychology2019a,
	title = {Recommendations {From} {Cognitive} {Psychology} for {Enhancing} the {Teaching} of {Natural}-{Science} {Categories}},
	volume = {6},
	issn = {2372-7322, 2372-7330},
