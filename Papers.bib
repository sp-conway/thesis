	author = {Zaki, Safa R. and Nosofsky, Robert M. and Jessup, Nenette M. and Unverzagt, Frederick W.},
	month = mar,
	year = {2003},
	pages = {394--406},
	file = {Zaki_et_al_2003.pdf:/Users/seanconway/Reading/Papers/Zaki_et_al_2003.pdf:application/pdf},
}

